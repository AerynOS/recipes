{
    "description": "RISCV64 UEFI for riscv64",
    "interface-types": [
        "uefi"
    ],
    "mapping": {
        "device": "flash",
        "executable": {
            "filename": "/usr/share/edk2/riscv64/RISCV_VIRT_CODE.fd",
            "format": "raw"
        },
        "nvram-template": {
            "filename": "/usr/share/edk2/riscv64/RISCV_VIRT_VARS.fd",
            "format": "raw"
        }
    },
    "targets": [
        {
            "architecture": "riscv64",
            "machines": [
                "virt",
                "virt-*"
            ]
        }
    ],
    "features": [
        "verbose-static"
    ],
    "tags": [

    ]
}
